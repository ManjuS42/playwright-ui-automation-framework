<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Playwright UI Suite" parallel="tests" thread-count="3">

    <listeners>
        <listener class-name="org.automation.playwrightframework.listeners.AllureTestListener"/>
    </listeners>

    <test name="Chromium Tests">
        <parameter name="browser" value="chromium"/>
        <classes>
            <class name="org.automation.playwrightframework.tests.LoginTest"/>
        </classes>
    </test>
</suite>
